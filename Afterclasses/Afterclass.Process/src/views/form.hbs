<form action="/logout" method="get">
<h1 style="display: inline-block;">Bienvenido {{user}}</h1> <button style="display: inline-block;" type="submit">Logout</button>
</form>

<h1>Carga tu producto</h1>

<form id="productForm">
    <input id="productInput" type="text" name="title" placeholder="Nombre de producto">
    <input id="priceInput" type="number" step="0.01" id="price" name="price" placeholder="Precio">
    <input id="imgInput" type="text" name="thumbnail" placeholder="Url de la imagen">
    <input type="submit">
</form>
<div id="productsContainer"></div>
